<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.*?>

<?import java.net.URL?>

<AnchorPane styleClass="backgroundColor" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="garage.LogController" onMouseClicked="#handleClick">
    <children>
        <TableView id="allSolversTable" fx:id="logTable" minHeight="-Infinity" prefHeight="175.0" prefWidth="200.0" AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="50.0">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
            <columns>
                <TableColumn fx:id="colAll" prefWidth="10.0" />
                <TableColumn fx:id="colDate" prefWidth="25.0" text="Date" />
                <TableColumn fx:id="colMiles" prefWidth="25.0" text="Miles" />
                <TableColumn fx:id="colItem" prefWidth="25.0" text="Item" />
                <TableColumn fx:id="colCompany" prefWidth="25.0" text="Company" />
                <TableColumn fx:id="colPrice" prefWidth="15.0" text="Price" />
                <TableColumn fx:id="colNotes" prefWidth="25.0" text="Notes" />
            </columns>
        </TableView>
    </children>
</AnchorPane>