<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.*?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import java.net.URL?>
<GridPane fx:id="myGrid" fx:controller="garage.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <!-- Headers -->
    <Label text="Last Change" GridPane.rowIndex="0" GridPane.columnIndex="1" GridPane.columnSpan="2" id="top"/>
    <!-- --><Label text="Date" GridPane.rowIndex="1" GridPane.columnIndex="1" id="header"/>
    <!-- --><Label text="Miles" GridPane.rowIndex="1" GridPane.columnIndex="2" id="header"/>
    <Label text="Limits" GridPane.rowIndex="0" GridPane.columnIndex="3" GridPane.columnSpan="2" id="top"/>
    <!-- --><Label text="Miles" GridPane.rowIndex="1" GridPane.columnIndex="3" id="header"/>
    <!-- --><Label text="Months" GridPane.rowIndex="1" GridPane.columnIndex="4" id="header"/>

    <!-- Categories and Items -->
    <!-- General -->
    <Label text="Inspect and Lubricate" fx:id="inspect" id="item" GridPane.rowIndex="2" GridPane.columnIndex="0" onMouseClicked="#handleClick" />
    <Label text="Replace Windshield Wipers" fx:id="replaceWipers" id="item" GridPane.rowIndex="3" GridPane.columnIndex="0" onMouseClicked="#handleClick" />

    <!-- Fluids -->
    <Label text="Oil and Filter Change" GridPane.rowIndex="4" GridPane.columnIndex="0" id="item" onMouseClicked="#handleClick" />
    <Label text="Replace CVT Fluid" GridPane.rowIndex="5" GridPane.columnIndex="0" id="item" onMouseClicked="#handleClick" />

    <!-- Tires -->
    <GridPane fx:id="tireRotation" GridPane.rowIndex="6" GridPane.columnIndex="0" GridPane.columnSpan="5" hgap="10" >
        <Label fx:id="title" text="Tire Rotation" id="item" GridPane.columnIndex="0" onMouseClicked="#handleClick" />
        <Label fx:id="dateOf" text="a" id="data" GridPane.columnIndex="1" />
        <Label fx:id="mileageOf" text="b" id="data" GridPane.columnIndex="2" />
        <Label fx:id="limitMiles" text="c" id="data" GridPane.columnIndex="3" />
        <Label fx:id="limitMonths"  text="d" id="data" GridPane.columnIndex="4" />
    </GridPane>
    <GridPane fx:id="newTires" GridPane.rowIndex="7" GridPane.columnIndex="0" GridPane.columnSpan="5"  hgap="10" >
        <Label text="New Tires" id="item" GridPane.columnIndex="0" onMouseClicked="#handleClick" />
        <Label text="a" id="data" GridPane.columnIndex="1" />
        <Label text="b" id="data" GridPane.columnIndex="2" />
        <Label text="c" id="data" GridPane.columnIndex="3" />
        <Label text="d" id="data" GridPane.columnIndex="4" />
    </GridPane>
    <Label text="Brake Fluid Change" GridPane.rowIndex="8" GridPane.columnIndex="0" id="item" onMouseClicked="#handleClick" />
    <Label text="New Brakes" GridPane.rowIndex="9" GridPane.columnIndex="0" id="item" onMouseClicked="#handleClick" />

    <!-- CSS -->
    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>

</GridPane>